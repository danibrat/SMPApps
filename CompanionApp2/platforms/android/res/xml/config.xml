<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sap.ide.companion" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="AppPreferences">
        <param name="android-package" value="com.sap.mp.cordova.plugins.apppreferences.AppPreferences" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="MAFLogonCoreCDVPluginJS" required="true">
        <param name="android-package" value="com.sap.maf.html5.android.MAFLogonCoreCDVPlugin" />
    </feature>
    <feature name="i18n">
        <param name="android-package" value="com.sap.mp.cordova.plugins.i18n.I18N" />
    </feature>
    <feature name="AppUpdate">
        <param name="android-package" value="com.sap.mp.cordova.plugins.appUpdate.AppUpdate" />
        <param name="onload" value="true" />
    </feature>
    <feature name="EncryptedStorage">
        <param name="android-package" value="com.sap.mp.cordova.plugins.storage.EncryptedStorage" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SMPPushPlugin" onload="true" required="true">
        <param name="android-package" value="com.sap.mp.push.PushPlugin" />
    </feature>
    <feature name="OData">
        <param name="android-package" value="com.sap.mp.cordova.plugins.odata.OData" />
    </feature>
    <feature name="E2ETrace">
        <param name="android-package" value="com.sap.mp.cordova.plugins.e2etrace.E2ETrace" />
    </feature>
    <preference name="showTitle" value="true" />
    <feature name="toolbar">
        <param name="android-package" value="com.sap.mp.cordova.plugins.toolbar.Toolbar" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="AuthProxy">
        <param name="android-package" value="com.sap.mp.cordova.plugins.authProxy.AuthProxy" />
        <param name="onload" value="true" />
    </feature>
    <preference name="SAPKapselHandleHttpRequests" value="false" />
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="Logging">
        <param name="android-package" value="com.sap.mp.cordova.plugins.logger.Logger" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="android-package" value="com.sap.mp.cordova.plugins.barcodescanner.BarcodeScanner" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Battery">
        <param name="android-package" value="org.apache.cordova.batterystatus.BatteryListener" />
    </feature>
    <feature name="Accelerometer">
        <param name="android-package" value="org.apache.cordova.devicemotion.AccelListener" />
    </feature>
    <feature name="Compass">
        <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="Media">
        <param name="android-package" value="org.apache.cordova.media.AudioHandler" />
    </feature>
    <feature name="Capture">
        <param name="android-package" value="org.apache.cordova.mediacapture.Capture" />
    </feature>
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="Vibration">
        <param name="android-package" value="org.apache.cordova.vibration.Vibration" />
    </feature>
    <feature name="Contacts">
        <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
    </feature>
    <feature name="Globalization">
        <param name="android-package" value="org.apache.cordova.globalization.Globalization" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>
    <name>WebIdeCompanion</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
