<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sap.ide.companion" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value=".25" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="TopActivityIndicator" value="gray" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="AppPreferences">
        <param name="ios-package" value="SMPAppPreferencesPlugin" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <feature name="AuthProxy">
        <param name="ios-package" value="SMPAuthProxyPlugin" />
        <param name="onload" value="true" />
    </feature>
    <preference name="SAPKapselHandleHttpRequests" value="true" />
    <feature name="MAFLogonCoreCDVPluginJS">
        <param name="ios-package" value="MAFLogonCoreCDVPlugin" />
    </feature>
    <feature name="i18n">
        <param name="ios-package" value="SMPI18nPlugin" />
    </feature>
    <feature name="AppUpdate">
        <param name="ios-package" value="SMPAppUpdatePlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="EncryptedStorage">
        <param name="ios-package" value="SMPEncryptedStoragePlugin" />
    </feature>
    <feature name="SMPPushPlugin" onload="true" required="true">
        <param name="ios-package" value="SMPPushPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="OData">
        <param name="ios-package" value="SMPODataPlugin" />
    </feature>
    <feature name="E2ETrace">
        <param name="ios-package" value="SMPE2ETracePlugin" />
    </feature>
    <feature name="toolbar">
        <param name="ios-package" value="SMPToolbarPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="Logging">
        <param name="ios-package" value="SMPLoggerPlugin" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="ios-package" value="SMPBarcodeScannerPlugin" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="Battery">
        <param name="ios-package" value="CDVBattery" />
    </feature>
    <feature name="Accelerometer">
        <param name="ios-package" value="CDVAccelerometer" />
    </feature>
    <feature name="Compass">
        <param name="ios-package" value="CDVCompass" />
    </feature>
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <preference name="CameraUsesGeolocation" value="false" />
    <feature name="Media">
        <param name="ios-package" value="CDVSound" />
    </feature>
    <feature name="Capture">
        <param name="ios-package" value="CDVCapture" />
    </feature>
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
    </feature>
    <feature name="Vibration">
        <param name="ios-package" value="CDVVibration" />
    </feature>
    <feature name="Contacts">
        <param name="ios-package" value="CDVContacts" />
    </feature>
    <feature name="Globalization">
        <param name="ios-package" value="CDVGlobalization" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
    </feature>
    <name>WebIdeCompanion</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="hybridapprevision" value="1" />
    <preference name="sap-development-version" value="1.2.3" />
</widget>
