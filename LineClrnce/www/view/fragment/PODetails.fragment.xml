<core:FragmentDefinition
	xmlns:l="sap.ui.layout"
	xmlns:model="sap.ui.model"
    xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<Table id="documentList" items="{/}">
	
		<columns>
			<Column minScreenWidth="Tablet" demandPopin="true"><Text text="Work Center" /></Column>
			<Column minScreenWidth="Tablet" demandPopin="true"><Text text="LC Status" /></Column>
			<Column minScreenWidth="Tablet" demandPopin="true"><Text text="Plant" /></Column>
			<Column minScreenWidth="Tablet" demandPopin="true"><Text text="Material" /></Column>
		</columns>
        <items>
            <ColumnListItem>
                <cells>
                    <Text text="{WorkCenter}" />
                    <Text text="{LCStatus}" />
                    <Text text="{Plant}" />
                    <Text text="{Material}" />
                    
                </cells>
            </ColumnListItem>
        </items>

	</Table>
</core:FragmentDefinition>